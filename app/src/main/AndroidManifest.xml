<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.finalyearprojectu">

    <uses-permission android:name="android.permission.CALL_PHONE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".donationManagement.ItemDonation"
            android:parentActivityName=".donationManagement.DonationMain"></activity>
        <activity
            android:name=".donationManagement.DonationMain"
            android:parentActivityName=".HomeDashBoard" />
        <activity
            android:name=".centersContactInformation.CentersAddresses"
            android:parentActivityName=".centersContactInformation.ContactCenters" />
        <activity
            android:name=".centersContactInformation.ContactCenters"
            android:parentActivityName=".HomeDashBoard" />
        <activity
            android:name=".missingPersonManagement.MissingPersonMain"
            android:parentActivityName=".HomeDashBoard" />
        <activity
            android:name=".edhiBloodBand.BloodBankMainActivity"
            android:parentActivityName=".HomeDashBoard" />
        <activity
            android:name=".AmbulanceDetail"
            android:parentActivityName=".AmbulanceActivity" />
        <activity
            android:name=".AmbulanceActivity"
            android:parentActivityName=".HomeDashBoard" />
        <activity
            android:name=".HomeDashBoard"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar" />
        <activity android:name=".sign_up"
            android:parentActivityName=".LogIn"/>
        <activity android:name=".LogIn">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>